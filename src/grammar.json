{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "magegamescript",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_root"
      }
    },
    "block_comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "/*"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "PATTERN",
              "value": "."
            }
          },
          {
            "type": "STRING",
            "value": "*/"
          }
        ]
      }
    },
    "line_comment": {
      "type": "TOKEN",
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "//"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "PATTERN",
                "value": "[^\\n]"
              }
            }
          ]
        }
      }
    },
    "QUOTED_STRING": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\"(?:[^\"\\\\]|\\\\.)*\""
      }
    },
    "quoted_string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "QUOTED_STRING"
        },
        {
          "type": "SYMBOL",
          "name": "CONSTANT"
        }
      ]
    },
    "quoted_string_expandable": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "quoted_string"
        },
        {
          "type": "SYMBOL",
          "name": "quoted_string_expansion"
        }
      ]
    },
    "quoted_string_expansion": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "quoted_string"
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "SYMBOL",
                        "name": "quoted_string"
                      }
                    ]
                  }
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "STRING",
                      "value": ","
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "CONSTANT": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\$[_a-zA-Z0-9]+"
      }
    },
    "_root": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "include_macro"
        }
      ]
    },
    "include_macro": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "include"
        },
        {
          "type": "FIELD",
          "name": "fileName",
          "content": {
            "type": "SYMBOL",
            "name": "quoted_string_expandable"
          }
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "block_comment"
    },
    {
      "type": "SYMBOL",
      "name": "line_comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [
    "quoted_string",
    "quoted_string_expandable"
  ],
  "supertypes": []
}
