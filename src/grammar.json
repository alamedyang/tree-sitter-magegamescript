{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "magegamescript",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_root"
      }
    },
    "block_comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "/*"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "PATTERN",
              "value": "."
            }
          },
          {
            "type": "STRING",
            "value": "*/"
          }
        ]
      }
    },
    "line_comment": {
      "type": "TOKEN",
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "//"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "PATTERN",
                "value": "[^\\n]"
              }
            }
          ]
        }
      }
    },
    "BAREWORD": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[_a-zA-Z][_a-zA-Z0-9]*"
      }
    },
    "QUOTED_STRING": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\"(?:[^\"\\\\]|\\\\.)*\""
      }
    },
    "STRING": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "QUOTED_STRING"
        },
        {
          "type": "SYMBOL",
          "name": "BAREWORD"
        }
      ]
    },
    "string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "STRING"
        },
        {
          "type": "SYMBOL",
          "name": "CONSTANT"
        }
      ]
    },
    "string_expandable": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "string_expansion"
        }
      ]
    },
    "string_expansion": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "string"
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "SYMBOL",
                        "name": "string"
                      }
                    ]
                  }
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "STRING",
                      "value": ","
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "DURATION": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[0-9]+(m?s)?"
      }
    },
    "duration": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "DURATION"
        },
        {
          "type": "SYMBOL",
          "name": "CONSTANT"
        }
      ]
    },
    "duration_expandable": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "duration"
        },
        {
          "type": "SYMBOL",
          "name": "duration_expansion"
        }
      ]
    },
    "duration_expansion": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "duration"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": ","
                  },
                  {
                    "type": "SYMBOL",
                    "name": "duration"
                  }
                ]
              }
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "STRING",
                  "value": ","
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          ]
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "CONSTANT": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\$[_a-zA-Z0-9]+"
      }
    },
    "_root": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "script_definition"
        }
      ]
    },
    "script_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "script"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "FIELD",
          "name": "script_name",
          "content": {
            "type": "SYMBOL",
            "name": "STRING"
          }
        },
        {
          "type": "SYMBOL",
          "name": "script_block"
        }
      ]
    },
    "script_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_script_item"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "_script_item": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_action_item"
            },
            {
              "type": "TOKEN",
              "content": {
                "type": "STRING",
                "value": ";"
              }
            }
          ]
        }
      ]
    },
    "_action_item": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "action_non_blocking_delay"
        }
      ]
    },
    "action_non_blocking_delay": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "wait"
        },
        {
          "type": "FIELD",
          "name": "duration",
          "content": {
            "type": "SYMBOL",
            "name": "duration_expandable"
          }
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "block_comment"
    },
    {
      "type": "SYMBOL",
      "name": "line_comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [
    "STRING",
    "string",
    "string_expandable",
    "duration",
    "duration_expandable"
  ],
  "supertypes": [],
  "reserved": {}
}