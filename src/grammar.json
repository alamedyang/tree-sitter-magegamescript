{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "magegamescript",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_root"
      }
    },
    "BAREWORD": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[_a-zA-Z][_a-zA-Z0-9]*"
      }
    },
    "_bareword": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "BAREWORD"
        },
        {
          "type": "SYMBOL",
          "name": "CONSTANT"
        }
      ]
    },
    "QUOTED_STRING": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\"(?:[^\"\\\\]|\\\\.)*\""
      }
    },
    "_quoted_string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "QUOTED_STRING"
        },
        {
          "type": "SYMBOL",
          "name": "CONSTANT"
        }
      ]
    },
    "_STRING": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "QUOTED_STRING"
        },
        {
          "type": "SYMBOL",
          "name": "BAREWORD"
        }
      ]
    },
    "_string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "QUOTED_STRING"
        },
        {
          "type": "SYMBOL",
          "name": "BAREWORD"
        },
        {
          "type": "SYMBOL",
          "name": "CONSTANT"
        }
      ]
    },
    "NUMBER": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[0-9]+"
      }
    },
    "_number": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "NUMBER"
        },
        {
          "type": "SYMBOL",
          "name": "CONSTANT"
        }
      ]
    },
    "DURATION": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[0-9]+(m?s)?"
      }
    },
    "_duration": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "DURATION"
        },
        {
          "type": "SYMBOL",
          "name": "CONSTANT"
        }
      ]
    },
    "DISTANCE": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[0-9]+(px|pix)?"
      }
    },
    "_distance": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "DISTANCE"
        },
        {
          "type": "SYMBOL",
          "name": "CONSTANT"
        }
      ]
    },
    "QUANTITY": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "once|twice|thrice|[0-9]+(x)?"
      }
    },
    "_quantity": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "QUANTITY"
        },
        {
          "type": "SYMBOL",
          "name": "CONSTANT"
        }
      ]
    },
    "COLOR": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "white|black|red|green|blue|magenta|cyan|yellow|#[0-9]{3,6}"
      }
    },
    "_color": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "COLOR"
        },
        {
          "type": "SYMBOL",
          "name": "CONSTANT"
        }
      ]
    },
    "_NUMBERISH": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "NUMBER"
        },
        {
          "type": "SYMBOL",
          "name": "DURATION"
        },
        {
          "type": "SYMBOL",
          "name": "DISTANCE"
        },
        {
          "type": "SYMBOL",
          "name": "QUANTITY"
        }
      ]
    },
    "_numberish": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_NUMBERISH"
        },
        {
          "type": "SYMBOL",
          "name": "CONSTANT"
        }
      ]
    },
    "BOOLEAN": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "true|false|on|off|open|closed"
      }
    },
    "_boolean": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "BOOLEAN"
        },
        {
          "type": "SYMBOL",
          "name": "CONSTANT"
        }
      ]
    },
    "CONSTANT": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\$[_a-zA-Z0-9]+"
      }
    },
    "ENUM_ALIGNMENT": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "STRING",
            "value": "TR"
          },
          {
            "type": "STRING",
            "value": "TL"
          },
          {
            "type": "STRING",
            "value": "TOP_RIGHT"
          },
          {
            "type": "STRING",
            "value": "TOP_LEFT"
          },
          {
            "type": "STRING",
            "value": "BR"
          },
          {
            "type": "STRING",
            "value": "BL"
          },
          {
            "type": "STRING",
            "value": "BOTTOM_RIGHT"
          },
          {
            "type": "STRING",
            "value": "BOTTOM_LEFT"
          }
        ]
      }
    },
    "_enum_alignment": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "ENUM_ALIGNMENT"
        },
        {
          "type": "SYMBOL",
          "name": "CONSTANT"
        }
      ]
    },
