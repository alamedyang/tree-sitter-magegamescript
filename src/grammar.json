{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "magegamescript",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_root"
      }
    },
    "block_comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "/*"
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "PATTERN",
              "value": "."
            }
          },
          {
            "type": "STRING",
            "value": "*/"
          }
        ]
      }
    },
    "line_comment": {
      "type": "TOKEN",
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "//"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "PATTERN",
                "value": "[^\\n]"
              }
            }
          ]
        }
      }
    },
    "BOOL": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "true|false|on|off|open|closed"
      }
    },
    "BAREWORD": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[_a-zA-Z][_a-zA-Z0-9]*"
      }
    },
    "QUOTED_STRING": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\"(?:[^\"\\\\]|\\\\.)*\""
      }
    },
    "STRING": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "QUOTED_STRING"
        },
        {
          "type": "SYMBOL",
          "name": "BAREWORD"
        }
      ]
    },
    "NUMBER": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[0-9]+"
      }
    },
    "DURATION": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[0-9]+(m?s)?"
      }
    },
    "duration": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "DURATION"
        },
        {
          "type": "SYMBOL",
          "name": "CONSTANT"
        }
      ]
    },
    "duration_expandable": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "duration"
        },
        {
          "type": "SYMBOL",
          "name": "duration_expansion"
        }
      ]
    },
    "duration_expansion": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "duration"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": ","
                  },
                  {
                    "type": "SYMBOL",
                    "name": "duration"
                  }
                ]
              }
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "STRING",
                  "value": ","
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          ]
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "DISTANCE": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "[0-9]+(px|pix)?"
      }
    },
    "QUANTITY": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "once|twice|thrice|[0-9]+(x)?"
      }
    },
    "NUMBERISH": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "NUMBER"
        },
        {
          "type": "SYMBOL",
          "name": "DURATION"
        },
        {
          "type": "SYMBOL",
          "name": "DISTANCE"
        },
        {
          "type": "SYMBOL",
          "name": "QUANTITY"
        }
      ]
    },
    "COLOR": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "white|black|red|green|blue|magenta|cyan|yellow|#[0-9]{3,6}"
      }
    },
    "CONSTANT": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "\\$[_a-zA-Z0-9]+"
      }
    },
    "CONSTANT_VALUE": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "STRING"
        },
        {
          "type": "SYMBOL",
          "name": "NUMBERISH"
        },
        {
          "type": "SYMBOL",
          "name": "BOOL"
        },
        {
          "type": "SYMBOL",
          "name": "COLOR"
        },
        {
          "type": "SYMBOL",
          "name": "CONSTANT"
        }
      ]
    },
    "_root": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "script_definition"
        },
        {
          "type": "SYMBOL",
          "name": "constant_assignment"
        }
      ]
    },
    "constant_assignment": {
      "type": "SEQ",
      "members": [
        {
          "type": "FIELD",
          "name": "label",
          "content": {
            "type": "SYMBOL",
            "name": "CONSTANT"
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "FIELD",
          "name": "value",
          "content": {
            "type": "SYMBOL",
            "name": "CONSTANT_VALUE"
          }
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "script_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "script"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "FIELD",
          "name": "script_name",
          "content": {
            "type": "SYMBOL",
            "name": "STRING"
          }
        },
        {
          "type": "SYMBOL",
          "name": "script_block"
        }
      ]
    },
    "script_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_script_item"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "_script_item": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_action_item"
            },
            {
              "type": "TOKEN",
              "content": {
                "type": "STRING",
                "value": ";"
              }
            }
          ]
        }
      ]
    },
    "_action_item": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "action_non_blocking_delay"
        }
      ]
    },
    "action_non_blocking_delay": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "wait"
        },
        {
          "type": "FIELD",
          "name": "duration",
          "content": {
            "type": "SYMBOL",
            "name": "duration_expandable"
          }
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "block_comment"
    },
    {
      "type": "SYMBOL",
      "name": "line_comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [
    "STRING",
    "duration",
    "duration_expandable",
    "NUMBERISH"
  ],
  "supertypes": []
}
