          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_STRING"
              },
              {
                "type": "SYMBOL",
                "name": "_NUMBERISH"
              },
              {
                "type": "SYMBOL",
                "name": "BOOLEAN"
              },
              {
                "type": "SYMBOL",
                "name": "COLOR"
              },
              {
                "type": "SYMBOL",
                "name": "CONSTANT"
              }
            ]
          }
        },
