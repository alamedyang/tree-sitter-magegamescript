{
  "name": "tree-sitter-magegamescript",
  "version": "2.0.0",
  "description": "A custom scripting DSL for a custom game engine on a custom PCB",
  "repository": "https://github.com/alamedyang",
  "license": "MIT",
  "author": {
    "name": "alamedyang",
    "email": "alamedyang@gmail.com"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "magegamescript"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.2.1",
    "node-gyp-build": "^4.8.2",
    "web-tree-sitter": "^0.25.3"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.24.6"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js",
	"regen": "tree-sitter generate && tree-sitter build --wasm"
  }
}
