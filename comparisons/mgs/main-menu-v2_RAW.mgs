   hex_control = false;
   if (warp_state == \"from-game\") { goto script main_menu-fade_in; }
   camera = geometry \"dc801-center\";
   camera fade in -> #000000 over 800ms;
   wait 2900ms;
   camera fade out -> #000000 over 800ms;
   player on_tick = null_script;
   goto main_menu-fade_in;
}

